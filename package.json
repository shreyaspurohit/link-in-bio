{
  "name": "link-in-bio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run --convention=nextjs -- next dev --turbopack --experimental-https",
    "build": "dotenvx run --convention=nextjs -- next build",
    "start": "dotenvx run --convention=nextjs -- next start",
    "lint": "dotenvx run --convention=nextjs -- next lint",
    "preview": "dotenvx run --convention=nextjs -- opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "dotenvx run --convention=nextjs --  opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"    
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@opennextjs/cloudflare": "^1.0.4",
    "@phosphor-icons/react": "^2.1.8",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.8.1",
    "clsx": "^2.1.1",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "tsparticles": "^3.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "phosphor-icons-tailwindcss": "^1.2.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.16.1"
  }
}
